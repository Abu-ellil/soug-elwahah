{
  "expo": {
    "name": "DRIVER",
    "slug": "DRIVER",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyDxtYXik5XAiuyINlwRI3MkJfbXJ4M7XsU"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDxtYXik5XAiuyINlwRI3MkJfbXJ4M7XsU"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to track deliveries.",
          "locationAlwaysPermission": "Allow $(PRODUCT_NAME) to use your location to track deliveries.",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to track deliveries."
        }
      ]
    ]
  }
}